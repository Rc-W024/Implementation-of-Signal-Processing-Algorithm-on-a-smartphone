<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.dsp.helloworld.MainActivity">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="L5_1(Offline)"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="#010000" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Compute"
        android:id="@+id/btncompute"
        android:layout_below="@+id/etd2value"
        android:layout_alignRight="@+id/tvresult"
        android:layout_alignEnd="@+id/tvresult" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/tvresult"
        android:textColor="#070000"
        android:layout_below="@+id/textView5"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="39dp"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/setpreference"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Settings"
        android:layout_below="@+id/etd2value"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/textView"
        android:layout_toStartOf="@+id/textView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Solution:"
        android:id="@+id/textView5"
        android:singleLine="false"
        android:textColor="#0a0101"
        android:layout_below="@+id/setpreference"
        android:layout_marginTop="35dp"
        android:layout_alignRight="@+id/textView7"
        android:layout_alignEnd="@+id/textView7" />

    <TextView
        android:id="@+id/setting_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="a value"
        android:id="@+id/textView2"
        android:layout_below="@+id/textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#100e0e" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/etavalue"
        android:text="1"
        android:layout_below="@+id/textView2"
        android:layout_alignRight="@+id/setpreference"
        android:layout_alignEnd="@+id/setpreference" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="a1 value"
        android:id="@+id/textView3"
        android:layout_alignTop="@+id/textView2"
        android:layout_toLeftOf="@+id/btncompute"
        android:layout_toStartOf="@+id/btncompute"
        android:textColor="#121212" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eta1value"
        android:text="1"
        android:layout_alignBottom="@+id/etavalue"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="a2 value"
        android:id="@+id/textView4"
        android:layout_below="@+id/etavalue"
        android:layout_alignRight="@+id/etavalue"
        android:layout_alignEnd="@+id/etavalue"
        android:layout_alignParentStart="true"
        android:textColor="#0c0a0a" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eta2value"
        android:text="1"
        android:layout_below="@+id/textView4"
        android:layout_alignRight="@+id/textView4"
        android:layout_alignEnd="@+id/textView4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="del1 value"
        android:id="@+id/textView6"
        android:layout_below="@+id/eta1value"
        android:layout_alignLeft="@+id/eta1value"
        android:layout_alignStart="@+id/eta1value"
        android:textColor="#0d0101" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="del2 value"
        android:id="@+id/textView7"
        android:layout_below="@+id/eta2value"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#100202" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="d1 value"
        android:id="@+id/textView8"
        android:layout_below="@+id/eta2value"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:textColor="#090101" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="d2 value"
        android:id="@+id/textView9"
        android:layout_above="@+id/etd2value"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#160606" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/etdel1value"
        android:text="0.1"
        android:layout_below="@+id/textView6"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/etd1value"
        android:text="0.1"
        android:layout_below="@+id/textView8"
        android:layout_alignRight="@+id/etdel1value"
        android:layout_alignEnd="@+id/etdel1value"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignStart="@+id/textView8" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/etd2value"
        android:text="0.2"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/setpreference"
        android:layout_alignEnd="@+id/setpreference" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/etdel2value"
        android:text="0.2"
        android:layout_alignBottom="@+id/etd1value"
        android:layout_alignRight="@+id/etd2value"
        android:layout_alignEnd="@+id/etd2value" />

</RelativeLayout>
